simu_2_model<- function(H,alpha,beta,gamma){
X<-NULL
X[1]<-1
for (i in 1:H){
proba<-c(X[i]*alpha/(X[i]*alpha+beta+gamma),beta/(X[i]*alpha+beta+gamma),gamma/(X[i]*alpha+beta+gamma))
